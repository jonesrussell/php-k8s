{
    "name": "jonesrussell/php-k8s",
    "description": "Control your Kubernetes clusters with this PHP-based Kubernetes client. It supports any form of authentication, the exec API, and it has an easy implementation for CRDs.",
    "keywords": [
        "laravel",
        "php",
        "kubernetes",
        "k8s",
        "k3s",
        "k0s",
        "cluster",
        "api",
        "kubeadm",
        "kubeapi",
        "kube"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/jonesrussell/php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        },
        {
            "name": "Russell Jones",
            "homepage": "https://github.com/jonesrussell",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-yaml": "*",
        "ext-curl": "*",
        "composer/semver": "^3.4",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/macroable": "^11.0",
        "illuminate/support": "^11.0",
        "ratchet/pawl": "^0.4.1",
        "symfony/process": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.22"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
